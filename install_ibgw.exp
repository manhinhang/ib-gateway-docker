#!/usr/bin/expect -f

set timeout -1

log_file /root/Jts/tws_install.log

spawn /tmp/ibgw.sh

expect "Where should IB Gateway $::env(IB_GATEWAY_MAJOR).$::env(IB_GATEWAY_MINOR) be installed?\r"

send -- "\r"

expect "Run IB Gateway $::env(IB_GATEWAY_MAJOR).$::env(IB_GATEWAY_MINOR)?\r"

send -- "n\r"

expect eof